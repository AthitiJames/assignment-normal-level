<h1>All Users</h1>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Birthday</th>
      <th>Gender</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Subject</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.birthday %></td>
        <td><%= user.gender %></td>
        <td><%= user.email %></td>
        <td><%= user.phone %></td>
        <td><%= user.subject %></td>
        <td>
          <%= link_to 'Edit', edit_user_path(user) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
